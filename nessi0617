-- [[ NESSI HUB FULL POWER EDITION : PART 1 ]]
-- Ê©üËÉΩ„Çí‰∏ÄÂàáÂ¶•Êâø„Åõ„Åö„ÄÅ„Éê„Ç∞ÂõûÈÅø„ÅÆ„Åü„ÇÅ„Å´Âá¶ÁêÜ„ÇíÂàÜÈõ¢„Åó„ÅüÂÆåÂÖ®Áâà

local LP = game:GetService("Players").LocalPlayer
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VU = game:GetService("VirtualUser")
local Cam = workspace.CurrentCamera

-- ÈáçË§áËµ∑ÂãïÈò≤Ê≠¢
if LP.PlayerGui:FindFirstChild("NessiFullPower") then LP.PlayerGui.NessiFullPower:Destroy() end

-- ÂÖ®„Å¶„ÅÆ„Éï„É©„Ç∞„Çí _G („Ç∞„É≠„Éº„Éê„É´) „Å´ÈÖçÁΩÆ„Åó„Å¶Á¢∫ÂÆü„Å´ÂÖ®„Çπ„ÇØ„É™„Éó„Éà„Åã„ÇâÂèÇÁÖß„Åï„Åõ„Çã
_G.NessiConfig = {
    -- ÊîªÊíÉÁ≥ª
    MapAttack = false,
    FastAttack = false,
    InstantKill = false,
    AttackRange = 5000,
    
    -- „Éï„Ç°„Éº„É†Á≥ª
    GhostShipFarm = false,
    SBFarm = false,
    AutoRaid = false,
    AutoFarm = false,
    
    -- ÁßªÂãï„Éª„Éì„Ç∏„É•„Ç¢„É´
    Hitbox = false,
    HitboxSize = 35,
    AirWalk = false,
    Fly = false,
    SpeedHack = false,
    StepSpeed = 2,
    
    -- „Éó„É¨„Ç§„É§„Éº
    StickActive = false,
    TargetPlayer = nil
}

-- --- „ÄêUIÊßãÁØâÔºö„Çπ„ÇØ„É≠„Éº„É´Âºè„Éï„É´„É°„Éã„É•„Éº„Äë ---
local sg = Instance.new("ScreenGui", LP.PlayerGui)
sg.Name = "NessiFullPower"
sg.ResetOnSpawn = false

local main = Instance.new("Frame", sg)
main.Size = UDim2.new(0, 300, 0, 450)
main.Position = UDim2.new(0.1, 0, 0.2, 0)
main.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
main.BorderSizePixel = 2
main.Active = true
main.Draggable = true -- Ëá™Áî±„Å´Âãï„Åã„Åõ„Çã„Çà„ÅÜ„Å´

-- „Çø„Ç§„Éà„É´
local title = Instance.new("TextLabel", main)
title.Size = UDim2.new(1, 0, 0, 40)
title.Text = "üêç NESSI HUB FULL POWER üêç"
title.TextColor3 = Color3.fromRGB(0, 255, 120)
title.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
title.Font = Enum.Font.GothamBold
title.TextSize = 16

-- „Çπ„ÇØ„É≠„Éº„É´„Ç®„É™„Ç¢
local scroll = Instance.new("ScrollingFrame", main)
scroll.Size = UDim2.new(1, -10, 1, -50)
scroll.Position = UDim2.new(0, 5, 0, 45)
scroll.BackgroundTransparency = 1
scroll.CanvasSize = UDim2.new(0, 0, 2.5, 0) -- È†ÖÁõÆ„ÅåÂ¢ó„Åà„Å¶„ÇÇÂ§ß‰∏àÂ§´„Å™„Çà„ÅÜ„Å´Èï∑„ÅèË®≠ÂÆö
scroll.ScrollBarThickness = 4

local layout = Instance.new("UIListLayout", scroll)
layout.Padding = UDim.new(0, 5)
layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

-- --- „Äê„Éú„Çø„É≥‰ΩúÊàêÁî®Èñ¢Êï∞„Äë ---
local function CreateToggle(name, configKey)
    local btn = Instance.new("TextButton", scroll)
    btn.Size = UDim2.new(0, 260, 0, 40)
    btn.Text = name .. " : OFF"
    btn.Font = Enum.Font.GothamSemibold
    btn.TextSize = 14
    btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    btn.TextColor3 = Color3.new(1, 1, 1)
    
    local corner = Instance.new("UICorner", btn)
    corner.CornerRadius = UDim.new(0, 6)
    
    btn.MouseButton1Click:Connect(function()
        _G.NessiConfig[configKey] = not _G.NessiConfig[configKey]
        if _G.NessiConfig[configKey] then
            btn.Text = name .. " : ON"
            btn.BackgroundColor3 = Color3.fromRGB(0, 150, 80)
        else
            btn.Text = name .. " : OFF"
            btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
        end
    end)
end

-- „Éú„Çø„É≥„ÇíÈ†ÜÊ¨°ÁôªÈå≤ÔºàUIË°®Á§∫Áî®Ôºâ
CreateToggle("ÂÖ®„Éû„ÉÉ„ÉóÊîªÊíÉ (Map Attack)", "MapAttack")
CreateToggle("Âç≥Ê≠ª (HP 0)", "InstantKill")
CreateToggle("È´òÈÄü„ÇØ„É™„ÉÉ„ÇØÊîªÊíÉ", "FastAttack")
CreateToggle("ÂπΩÈúäËàπÁ¥†ÊùêÈõÜ„ÇÅ", "GhostShipFarm")
CreateToggle("„Ç∑„Éº„Éì„Éº„Çπ„ÉàËá™ÂãïÁã©„Çä", "SBFarm")
CreateToggle("Ëá™Âãï„É¨„Ç§„Éâ„ÇØ„É™„Ç¢", "AutoRaid")
CreateToggle("NPC„Éí„ÉÉ„Éà„Éú„ÉÉ„ÇØ„ÇπÊã°Â§ß", "Hitbox")
CreateToggle("Á©∫‰∏≠Ê≠©Ë°å (Air Walk)", "AirWalk")
CreateToggle("Ë∂≥Âä†ÈÄü (Speed Hack)", "SpeedHack")

-- Ë°®Á§∫/ÈùûË°®Á§∫Ë®≠ÂÆö (RightControl)
UIS.InputBegan:Connect(function(input, gp)
    if not gp and input.KeyCode == Enum.KeyCode.RightControl then
        main.Visible = not main.Visible
    end
end)

print("Part 1 Loaded. Waiting for Part 2...")-- [[ NESSI HUB FULL POWER EDITION : PART 2 ]]
-- „É°„Ç§„É≥„É≠„Ç∏„ÉÉ„ÇØÔºöÊîªÊíÉ„ÄÅÂç≥Ê≠ª„ÄÅ„ÉÜ„É¨„Éù„ÄÅÂ∑®Â§ßÂåñ„ÅÆÂÆüË°å

task.spawn(function()
    local lastFastAttack = 0
    
    while task.wait() do
        pcall(function()
            local char = LP.Character
            if not char or not char:FindFirstChild("HumanoidRootPart") then return end
            local root = char.HumanoidRootPart

            -- --- „Äê1. ÊîªÊíÉ„ÉªÂç≥Ê≠ª„ÉªÂÖ®„Éû„ÉÉ„ÉóÂà§ÂÆö„Äë ---
            -- ÊèèÁîªÁØÑÂõ≤Â§ñ„ÅÆÊïµ„Å´„ÇÇ„Éë„Ç±„ÉÉ„Éà„ÇíÈ£õ„Å∞„Åô„Éï„É´„Éë„ÉØ„Éº‰ªïÊßò
            if _G.NessiConfig.MapAttack or _G.NessiConfig.InstantKill or _G.NessiConfig.FastAttack then
                -- workspace.Enemies (Blox Fruits„ÅÆÊïµ„Éï„Ç©„É´„ÉÄ) „ÇíÂæπÂ∫ïËµ∞Êüª
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                        
                        -- Âç≥Ê≠ªË®≠ÂÆö
                        if _G.NessiConfig.InstantKill then
                            v.Humanoid.Health = 0
                        end

                        -- ÂÖ®„Éû„ÉÉ„ÉóÊîªÊíÉ / È´òÈÄüÊîªÊíÉ (RegisterHit„ÇíÁõ¥Êé•„Çµ„Éº„Éê„Éº„Å´Âè©„ÅçËæº„ÇÄ)
                        if _G.NessiConfig.MapAttack or _G.NessiConfig.FastAttack then
                            -- ÂºïÊï∞Ôºö(ÂØæË±°„ÅÆ„Éë„Éº„ÉÑ, {{ÂØæË±°„ÅÆ„É¢„Éá„É´, ÂØæË±°„ÅÆ„Éë„Éº„ÉÑ}})
                            -- „Åì„Çå„Å´„Çà„ÇäË∑ùÈõ¢„ÅåÈõ¢„Çå„Å¶„ÅÑ„Å¶„ÇÇ„ÄåÂΩì„Åü„Å£„Åü„Äç„Å®Âà§ÂÆö„Åï„Åõ„Çã
                            ReplicatedStorage.Modules.Net["RE/RegisterHit"]:FireServer(v.HumanoidRootPart, {{v, v.HumanoidRootPart}})
                        end
                    end
                end
            end

            -- --- „Äê2. NPC„Éí„ÉÉ„Éà„Éú„ÉÉ„ÇØ„ÇπÊã°Â§ß„Äë ---
            -- „Éó„É¨„Ç§„É§„Éº„ÅØÁÑ°Ë¶ñ„Åó„Å¶„ÄÅEnemies„Éï„Ç©„É´„ÉÄ„ÅÆNPC„Å†„Åë„ÇíÂØæË±°„Å´„Åô„Çã
            if _G.NessiConfig.Hitbox then
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("HumanoidRootPart") then
                        v.HumanoidRootPart.Size = Vector3.new(_G.NessiConfig.HitboxSize, _G.NessiConfig.HitboxSize, _G.NessiConfig.HitboxSize)
                        v.HumanoidRootPart.Transparency = 0.8
                        v.HumanoidRootPart.CanCollide = false
                    end
                end
            else
                -- „Ç™„Éï„ÅÆÊôÇ„ÅØ„Çµ„Ç§„Ç∫„ÇíÊàª„ÅôÂá¶ÁêÜÔºà„Éê„Ç∞Èò≤Ê≠¢Ôºâ
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("HumanoidRootPart") and v.HumanoidRootPart.Size.X > 5 then
                        v.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
                        v.HumanoidRootPart.Transparency = 0
                    end
                end
            end

            -- --- „Äê3. ÂπΩÈúäËàπ & „Ç∑„Éº„Éì„Éº„Çπ„ÉàËá™ÂãïËøΩÂ∞æ„Äë ---
            -- ÂπΩÈúäËàπÁ¥†ÊùêÈõÜ„ÇÅ
            if _G.NessiConfig.GhostShipFarm then
                for _, v in pairs(workspace:GetChildren()) do
                    -- ÂπΩÈúäËàπ„ÅÆ„É¢„Éá„É´Âêç„ÇíÁ∂≤ÁæÖ
                    if (v.Name == "ShipGroup" or v.Name == "GhostShip" or v.Name == "Ship") and v:FindFirstChildWhichIsA("BasePart") then
                        -- Ëàπ„ÅÆÂ∞ë„Åó‰∏ä„Å´Âõ∫ÂÆö„Åó„Å¶„ÄÅÊîªÊíÉÂà§ÂÆö„ÇíÂèó„Åë„Å™„ÅÑ„Çà„ÅÜ„Å´„Åó„Å§„Å§MapAttack„ÅßÂ£ä„Åô
                        root.CFrame = v:FindFirstChildWhichIsA("BasePart").CFrame * CFrame.new(0, 60, 0)
                    end
                end
            
            -- „Ç∑„Éº„Éì„Éº„Çπ„Éà (SB) Áã©„Çä
            elseif _G.NessiConfig.SBFarm then
                for _, v in pairs(workspace:GetChildren()) do
                    if v.Name == "Sea Beast" and v:FindFirstChild("HumanoidRootPart") then
                        -- SB„ÅÆÈ†≠‰∏ä„Å´Âõ∫ÂÆö
                        root.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 50, 0)
                    end
                end
            end

            -- --- „Äê4. Ëá™Âãï„É¨„Ç§„Éâ„Äë ---
            if _G.NessiConfig.AutoRaid then
                -- „É¨„Ç§„Éâ‰∏≠„ÅÆÊïµ„ÇíÊúÄÂÑ™ÂÖà„ÅßÂÖ®ÊªÖ„Åï„Åõ„Çã
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        root.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
                        v.Humanoid.Health = 0
                    end
                end
            end

        end)
    end
end)

print("Part 2 Loaded. Moving to Part 3 (Movement)...")-- [[ NESSI HUB FULL POWER EDITION : PART 2 (Update) ]]

task.spawn(function()
    while task.wait() do
        pcall(function()
            local char = LP.Character
            if not char or not char:FindFirstChild("HumanoidRootPart") then return end
            local root = char.HumanoidRootPart

            -- --- „Äê1. ÊîªÊíÉ„ÉªÂç≥Ê≠ª„ÉªÂÖ®„Éû„ÉÉ„ÉóÂà§ÂÆö„Äë ---
            if _G.NessiConfig.MapAttack or _G.NessiConfig.InstantKill or _G.NessiConfig.FastAttack then
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                        if _G.NessiConfig.InstantKill then v.Humanoid.Health = 0 end
                        if _G.NessiConfig.MapAttack or _G.NessiConfig.FastAttack then
                            ReplicatedStorage.Modules.Net["RE/RegisterHit"]:FireServer(v.HumanoidRootPart, {{v, v.HumanoidRootPart}})
                        end
                    end
                end
            end

            -- --- „Äê2. NPC„Éí„ÉÉ„Éà„Éú„ÉÉ„ÇØ„ÇπÊã°Â§ß„Äë ---
            if _G.NessiConfig.Hitbox then
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("HumanoidRootPart") then
                        v.HumanoidRootPart.Size = Vector3.new(_G.NessiConfig.HitboxSize, _G.NessiConfig.HitboxSize, _G.NessiConfig.HitboxSize)
                        v.HumanoidRootPart.Transparency = 0.8
                        v.HumanoidRootPart.CanCollide = false
                    end
                end
            end

            -- --- „Äê3. ÂπΩÈúäËàπ & „Ç∑„Éº„Éì„Éº„Çπ„Éà„Äë ---
            if _G.NessiConfig.GhostShipFarm then
                for _, v in pairs(workspace:GetChildren()) do
                    if (v.Name == "ShipGroup" or v.Name == "GhostShip") and v:FindFirstChildWhichIsA("BasePart") then
                        root.CFrame = v:FindFirstChildWhichIsA("BasePart").CFrame * CFrame.new(0, 60, 0)
                    end
                end
            elseif _G.NessiConfig.SBFarm then
                for _, v in pairs(workspace:GetChildren()) do
                    if v.Name == "Sea Beast" and v:FindFirstChild("HumanoidRootPart") then
                        root.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 50, 0)
                    end
                end
            end

            -- --- „Äê4. Ëá™Âãï„É¨„Ç§„Éâ„Äë ---
            if _G.NessiConfig.AutoRaid then
                for _, v in pairs(workspace.Enemies:GetChildren()) do
                    if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        root.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 20, 0)
                        v.Humanoid.Health = 0
                    end
                end
            end

            -- --- „Äê„Åì„Åì„Å´„Éó„É¨„Ç§„É§„ÉºÈÅ∏ÊäûÊõ¥Êñ∞„ÇíËøΩÂä†ÔºÅ„Äë ---
            -- ‚Äª UIÂÅ¥„ÅÆ playerScroll „Åå‰ΩúÊàê„Åï„Çå„Å¶„ÅÑ„ÇãÂâçÊèê
            if _G.LastRefresh == nil or tick() - _G.LastRefresh > 30 then
                _G.LastRefresh = tick()
                pcall(function()
                    -- UIÂÅ¥„ÅÆ„É™„Çπ„Éà„Çí„ÇØ„É™„Ç¢„Åó„Å¶ÂÜçÊßãÁØâ
                    if playerScroll then
                        for _, child in pairs(playerScroll:GetChildren()) do
                            if child:IsA("TextButton") then child:Destroy() end
                        end
                        for _, p in pairs(game:GetService("Players"):GetPlayers()) do
                            if p ~= LP then
                                local pBtn = Instance.new("TextButton", playerScroll)
                                pBtn.Size = UDim2.new(1, -5, 0, 30)
                                pBtn.Text = p.DisplayName
                                pBtn.BackgroundColor3 = (_G.NessiConfig.TargetPlayer == p) and Color3.fromRGB(100, 50, 150) or Color3.fromRGB(45, 45, 55)
                                pBtn.TextColor3 = Color3.new(1, 1, 1)
                                pBtn.MouseButton1Click:Connect(function()
                                    _G.NessiConfig.TargetPlayer = p
                                end)
                            end
                        end
                    end
                end)
            end

        end)
    end
end)
